{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
     
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
           "plugins": ["react-hooks"],
    
    "rules": {
        "semi": [2, "always"],
    "react-hooks/rules-of-hooks": "error",
         "react/prop-types": "off"
      
    }
}
